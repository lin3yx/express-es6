{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "no-console": "off",
    "quotes": ["error", "single"],
    "object-curly-newline": ["error", { "multiline": true }]
  }
}
